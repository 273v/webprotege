<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .main {
            height: 22px;
            line-height: 21px;
            cursor: pointer;
        }
        .main:hover {
            background-color: #f0f0f0;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{style.main}">
        <g:Label ui:field="displayNameField"/>
    </g:HTMLPanel>
</ui:UiBinder>