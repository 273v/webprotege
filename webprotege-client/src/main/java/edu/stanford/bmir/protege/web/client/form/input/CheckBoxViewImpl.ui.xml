<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field="cb" type="edu.stanford.bmir.protege.web.client.form.input.CheckBoxResourceBundle"/>
    <g:HTMLPanel addStyleNames="{cb.style.checkBox}">
        <g:HTMLPanel ui:field="input" addStyleNames="{cb.style.checkBox__input}" width="16px" height="16px">
            <svg width="16px" height="16px">
                <rect class="wp-checkbox__input__box" x="1" y="1" width="14" height="14" rx="2" ry="2"/>
                <path class="wp-checkbox__input__check-mark" d="M3 8 L7 12 L13 4" fill="none" stroke-linecap="square"/>
            </svg>
        </g:HTMLPanel>
        <g:Label ui:field="label" addStyleNames="{cb.style.checkBox__label}"/>
    </g:HTMLPanel>
</ui:UiBinder>