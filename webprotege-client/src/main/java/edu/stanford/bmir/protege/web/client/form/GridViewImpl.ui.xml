<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:editor="urn:import:edu.stanford.bmir.protege.web.client.editor">
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .limitedValuesMessage {
            color: literal("var(--tertiary--color)");
            background-color: rgb(255, 250, 230);
            padding: 4px;
        }
    </ui:style>
    <g:HTMLPanel addStyleNames="{wp.style.formGrid}">
        <g:SimplePanel ui:field="headerContainer"/>
        <editor:ValueListEditor ui:field="rowEditor" enabled="true" newRowMode="MANUAL"/>
        <g:Label ui:field="limitedRowsDisplayedMessage" visible="false" addStyleNames="{style.limitedValuesMessage}"/>
    </g:HTMLPanel>
</ui:UiBinder>
