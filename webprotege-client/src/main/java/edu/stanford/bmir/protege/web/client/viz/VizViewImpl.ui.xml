<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style>
        @external .node;
        @external .edge;
        .node text, .edge text {
            font-family: inherit;
            font-size: 8px;
        }
        .edge text {
        }
        .edge path {
            stroke-width: 0.5;
        }
        .edge polygon {
            stroke-linejoin: round;
            stroke-linecap: round;
            stroke-width: 1;
        }
        .node polygon {
            stroke-width: 0.6;
            stroke-linejoin: round;
        }
        .container {
            touch-action: pinch-zoom;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTML ui:field="imageContainer" addStyleNames="{style.container}"/>
    </g:HTMLPanel>
</ui:UiBinder>