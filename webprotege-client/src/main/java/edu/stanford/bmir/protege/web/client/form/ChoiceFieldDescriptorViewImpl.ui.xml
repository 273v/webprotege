<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:editor="urn:import:edu.stanford.bmir.protege.web.client.editor">
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup}">
            <g:Label addStyleNames="{wp.style.formLabel}" text="Widget Type"/>
            <g:RadioButton name="widgetType" text="Segmented radio button" ui:field="segmentedRadio"/>
            <g:RadioButton name="widgetType" text="Radio button" ui:field="radioRadio"/>
            <g:RadioButton name="widgetType" text="Check box" ui:field="checkBoxRadio"/>
        </g:HTMLPanel>
        <g:HTMLPanel addStyleNames="{wp.style.formGroup}">
            <editor:ValueListEditor ui:field="choiceListEditor"/>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>
