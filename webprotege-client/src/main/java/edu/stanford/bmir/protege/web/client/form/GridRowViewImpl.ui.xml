<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .row {
            display: flex;
            flex-direction: row;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel ui:field="rowContainer" addStyleNames="{style.row}"/>
    </g:HTMLPanel>
</ui:UiBinder>
